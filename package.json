{
    "name": "chaar-dham-fe",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "start": "vite --mode development",
        "start:dev": "vite --mode development",
        "start:staging": "vite --mode staging",
        "start:production": "vite --mode production",
        "build": "tsc && vite build",
        "build:dev": "tsc && vite build --mode development",
        "build:staging": "tsc && vite build --mode staging",
        "build:production": "tsc && vite build --mode production",
        "generate-pwa-assets": "pwa-assets-generator --preset minimal public/logo.svg",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prettier": "prettier src -check",
        "lint-staged": "lint-staged --config lint-staged.js",
        "husky-install": "husky install",
        "format-check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,html,css,scss,md}\"",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,html,css,scss,md}\"",
        "serve": "http-server dist -p 3000"
    },
    "dependencies": {
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.5",
        "@mui/material": "^5.15.5",
        "@popperjs/core": "^2.11.8",
        "@reduxjs/toolkit": "^2.0.1",
        "axios": "^1.6.5",
        "bcryptjs": "^2.4.3",
        "crypto-hash": "^3.0.0",
        "crypto-js": "^4.2.0",
        "formik": "^2.4.5",
        "i18next": "^23.7.16",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^14.0.0",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.21.2",
        "redux-saga": "^1.3.0",
        "sass": "^1.69.7",
        "styled-components": "^6.1.12",
        "uuid": "^9.0.1",
        "yup": "^1.3.3"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/crypto-js": "^4.2.2",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vite-pwa/assets-generator": "^0.2.4",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^8.56.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^8.0.3",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.2",
        "tsc-files": "^1.1.4",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-pwa": "^0.20.0"
    }
}
